<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
   assembly="DYDP.Domain"
   namespace="DYDP.Domain.Entities">

  <class name ="ticketinfo">
    <id name="ticketid" column ="ticketid">
      <generator class ="native"/>
    </id>
    <property name ="moviename"/>
    <property name ="starttime"/>
    <property name ="seatid"/>
    <property name ="hallid"/>
    <property name ="movietime"/>
    <property name ="price"/>
    <property name ="seller"/>
  </class>
</hibernate-mapping> 
